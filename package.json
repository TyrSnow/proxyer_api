{
  "name": "proxyer_api",
  "version": "0.1.0",
  "scripts": {
    "init": "cnpm install",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.deploy.json",
    "pretest": "mongorestore -d test_proxyer db/dump/test_proxyer",
    "test": "set NODE_ENV=test&&set CLUSTER=disabled&& nyc mocha -r ./node_modules/ts-node/register -r ./node_modules/source-map-support/register api/**/*.spec.ts",
    "test-ci": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.schemas.ts",
      "**/*.schema.ts",
      "**/*.model.ts",
      "**/*.enum.ts",
      "api/model/*.ts",
      "api/constants/*.ts",
      "**/server.ts",
      "**/*.js"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  },
  "dependencies": {
    "@types/query-string": "^6.1.0",
    "@types/v8-profiler": "^0.0.5",
    "async": "^2.6.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-jsonschema": "^1.1.6",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "log4js": "^2.3.11",
    "mongoose": "^5.0.15",
    "pinyin": "^2.8.3",
    "query-string": "^6.1.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "stream-throttle": "^0.1.3",
    "v8-profiler": "^5.7.0",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/body-parser": "^1.16.7",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.39",
    "@types/express-jwt": "^0.0.38",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/log4js": "^2.3.5",
    "@types/mocha": "^2.2.44",
    "@types/mongoose": "^4.7.24",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.47",
    "@types/redis": "^2.8.1",
    "@types/supertest": "^2.0.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "istanbul-harmony": "^0.3.16",
    "mocha": "^5.0.4",
    "multer": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "source-map-support": "^0.5.4",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "license": "MIT",
  "author": {
    "name": "Tian Yu",
    "email": "tianyu@juxinli.com"
  }
}
